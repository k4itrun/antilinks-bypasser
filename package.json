{
  "name": "antilinks-bypasser",
  "description": "No discord anti link system will be able to delete your URLs with this script.",
  "version": "1.0.0",
  "exports": null,
  "type": "module",
  "scripts": {
    "deploy": "tsx src/index.ts",
    "hook:init": "lefthook install",
    "ci:release": "semantic-release",
    "ci:commitlint": "commitlint --from=HEAD~10",
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k4itrun/antilinks-bypasser.git"
  },
  "keywords": [],
  "author": {
    "name": "k4itrun",
    "email": "billoneta@proto.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k4itrun/antilinks-bypasser/issues"
  },
  "homepage": "https://github.com/k4itrun/antilinks-bypasser#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@dotenvx/dotenvx": "^1.42.0",
    "@k4i/config": "^1.0.2",
    "@k4i/utils": "^1.0.0",
    "@types/node": "^22.15.3",
    "eslint": "^9.25.1",
    "lefthook": "^1.11.12",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.10.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
